<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="reveal">
        <attr name="type" format="enum">
            <enum name="activity" value="0"/>
            <enum name="dialog" value="1"/>
        </attr>
        <attr name="location" format="enum">
            <enum name="top_left" value="1"/>
            <enum name="top" value="2"/>
            <enum name="top_right" value="3"/>
            <enum name="left" value="4"/>
            <enum name="center" value="5"/>
            <enum name="right" value="6"/>
            <enum name="bottom_left" value="7"/>
            <enum name="bottom" value="8"/>
            <enum name="bottom_right" value="9"/>
        </attr>
        <attr name="x" format="integer"/>
        <attr name="y" format="integer"/>
        <attr name="duration_open" format="integer"/>
        <attr name="duration_exit" format="integer"/>
    </declare-styleable>
</resources>