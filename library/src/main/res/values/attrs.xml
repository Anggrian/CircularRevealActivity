<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularRevealLayout">
        <attr name="reveal_location" format="enum">
            <enum name="top_left" value="1"/>
            <enum name="top" value="2"/>
            <enum name="top_right" value="3"/>
            <enum name="left" value="4"/>
            <enum name="center" value="5"/>
            <enum name="right" value="6"/>
            <enum name="bottom_left" value="7"/>
            <enum name="bottom" value="8"/>
            <enum name="bottom_right" value="9"/>
        </attr>
        <attr name="reveal_exit_duration" format="integer"/>
        <attr name="reveal_open_duration" format="integer"/>
    </declare-styleable>
</resources>